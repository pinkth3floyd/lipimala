{
  "functions": {
    "src/app/actions/optimizedActions.ts": {
      "maxDuration": 300
    }
  },
  "env": {
    "HUGGINGFACE_CACHE_DIR": "/tmp/huggingface_cache",
    "NODE_OPTIONS": "--max-old-space-size=3000"
  },
  "build": {
    "env": {
      "HUGGINGFACE_CACHE_DIR": "/tmp/huggingface_cache",
      "NODE_OPTIONS": "--max-old-space-size=3000"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
